  "devDependencies": {
    "browserify": "^14.3.0",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "istanbul": "^0.4.5",
    "istanbul-coveralls": "^1.0.3",
    "mocha": "^3.2.0",
    "semver": "^5.5.0",
